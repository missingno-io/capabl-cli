{
    "name": "missingno/capabl-cli",
    "description": "A CLI tool for streamlined wordpress development",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "wordpress", "bullseye", "capabl"],
    "homepage": "https://wordpress.capabl.io",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Stage",
            "email": "cj.stage@gmail.com"
        },
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laminas/laminas-text": "^2.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3",
        "laravel-zero/framework": "^8.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/capabl"]
}
